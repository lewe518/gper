<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="com.gupao.spring.pojo.TeacherBeanImpl" id="bean1">
        <constructor-arg name="name" value="zhang san"/>
        <constructor-arg name="age" value="10"/>
    </bean>

    <bean class="com.gupao.spring.pojo.TeacherBeanImpl" c:_0="zhang san" c:_1="10" id="bean2"/>

    <bean class="com.gupao.spring.pojo.TeacherBeanImpl" id="bean3">
        <property name="name" value="zhang san"/>
        <property name="age" value="10"/>
    </bean>

</beans>